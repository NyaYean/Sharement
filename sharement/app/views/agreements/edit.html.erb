<%= form_for @agreement do |f| %>
	<%= f.label :name %>
	<br>
	<%= f.text_field :name %>
	<br>
	<%= f.label :description %>
	<br>
	<%= f.text_field :description %>
	<br>
	<%= f.label :start_date %>
	<br>
	<%= f.text_field :start_date %>
	<br>
	<%= f.label :agreement_details %>
	<br>
	<%= f.text_field :agreement_details %>
	<br>
	<%= f.label :payer %>
	<br>
	<%= f.text_field :payer %>
	<br>
	<%= f.submit %>

<% end %>