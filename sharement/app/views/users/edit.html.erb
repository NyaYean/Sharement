<%= form_for @user do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>
	
	<%= f.label :description %>
	<%= f.text_field :description %>
	<br>

	<%= f.label :start_date %>
	<%= f.text_field :start_date %>
	<br>

	<%= f.label :agreement_details %>
	<%= f.text_field :agreement_details %>
	<br>

	<%= f.label :payer %>
	<%= f.text_field :payer %>
	<br>

	<%= f.submit %>
<% end %>


